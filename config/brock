server{
    server_name brock-imex.com;
    client_max_body_size 100M;
    location /media {
        alias /home/jurgeon/projects/brock/src/media;
    }
    location /static {
        alias /home/jurgeon/projects/brock/src/static_root;
    }
    location / {
        uwsgi_pass unix:/home/jurgeon/projects/brock/src/config/brock.sock;
        include /etc/nginx/uwsgi_params;
    }




    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/brock-imex.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/brock-imex.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}

server{
    server_name www.brock-imex.com;
    client_max_body_size 100M;
    location /media {
        alias /home/jurgeon/projects/brock/src/media;
    }
    location /static {
        alias /home/jurgeon/projects/brock/src/static_root;
    }
    location / {
        uwsgi_pass unix:/home/jurgeon/projects/brock/src/config/brock.sock;
        include /etc/nginx/uwsgi_params;
    }


}
server{
    if ($host = brock-imex.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    server_name brock-imex.com;
    listen 80;
    return 404; # managed by Certbot


}