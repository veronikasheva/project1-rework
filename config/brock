server{
    server_name brock.in.ua;
    client_max_body_size 100M;
    location /media {
        alias /home/jurgeon/projects/brock/src/media;
    }
    location /static {
        alias /home/jurgeon/projects/brock/src/static_root;
    }
    location / {
        uwsgi_pass unix:/home/jurgeon/projects/brock/src/config/brock.sock;
        include /etc/nginx/uwsgi_params;
    }


server{
    server_name www.brock.in.ua;
    client_max_body_size 100M;
    location /media {
        alias /home/jurgeon/projects/brock/src/media;
    }
    location /static {
        alias /home/jurgeon/projects/brock/src/static_root;
    }
    location / {
        uwsgi_pass unix:/home/jurgeon/projects/brock/src/config/brock.sock;
        include /etc/nginx/uwsgi_params;
    }